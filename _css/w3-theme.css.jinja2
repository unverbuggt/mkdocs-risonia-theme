.w3-theme-l5 {color:#000 !important; background-color:{{ color_l5 }} !important}
.w3-theme-l4 {color:#000 !important; background-color:{{ color_l4 }} !important}
.w3-theme-l3 {color:#000 !important; background-color:{{ color_l3 }} !important}
.w3-theme-l2 {color:#000 !important; background-color:{{ color_l2 }} !important}
.w3-theme-l1 {color:#000 !important; background-color:{{ color_l1 }} !important}

.w3-theme-dark {color:#fff !important; background-color:{{ color_d5 }} !important}
.w3-theme-light {color:#000 !important; background-color:{{ color_l5 }} !important}
.w3-theme-action {color:#fff !important; background-color:{{ color_d5 }} !important}

.w3-theme {color:#000 !important; background-color:{{ color_theme }} !important}
.w3-text-theme {color:{{ color_theme }} !important}
.w3-border-theme {border-color:{{ color_theme }} !important}

.w3-hover-theme:hover {color:#000 !important; background-color:{{ color_theme }} !important}
.w3-hover-text-theme:hover {color:{{ color_theme }} !important}
.w3-hover-border-theme:hover {border-color:{{ color_theme }} !important}
pre {
  color:#000;
  background-color:{{ color_l4 }};
  border-left:4px solid {{ color_theme }};
}
code {
  background-color:{{ color_l4 }};
}
a{color:{{ color_theme }} !important}
table tbody tr:hover,table li:hover{
    background-color:{{ color_l3 }}
}

@media (prefers-color-scheme: dark) {
  .w3-theme-l1 {color:#fff !important; background-color:{{ color_d1 }} !important}
  .w3-theme-l2 {color:#fff !important; background-color:{{ color_d2 }} !important}
  .w3-theme-l3 {color:#fff !important; background-color:{{ color_d3 }} !important}
  .w3-theme-l4 {color:#fff !important; background-color:{{ color_d4 }} !important}
  .w3-theme-l5 {color:#fff !important; background-color:{{ color_d5 }} !important}

  .w3-theme-dark {color:#000 !important; background-color:{{ color_l5 }} !important}
  .w3-theme-light {color:#fff !important; background-color:{{ color_d5 }} !important}
  .w3-theme-action {color:#fff !important; background-color:{{ color_d5 }} !important}

  .w3-theme {color:#fff !important; background-color:{{ color_theme }} !important}
  .w3-text-theme {color:{{ color_theme }} !important}
  .w3-border-theme {border-color:{{ color_theme }} !important}

  .w3-hover-theme:hover {color:#fff !important; background-color:{{ color_theme }} !important}
  .w3-hover-text-theme:hover {color:{{ color_theme }} !important}
  .w3-hover-border-theme:hover {border-color:{{ color_theme }} !important}
  pre {
    color:#fff;
    background-color:{{ color_d4 }};
    border-left:4px solid {{ color_theme }};
  }
  code {
    background-color:{{ color_d4 }};
  }
  a{color:{{ color_theme }} !important}
  table tbody tr:hover,table li:hover{
    background-color:{{ color_d3 }}
  }
}